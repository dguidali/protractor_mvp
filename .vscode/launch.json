    "version": "0.2.0",
    "configurations": [

        {
            "name": "Cucumber",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceRoot}",
            "program": "${workspaceRoot}/node_modules/protractor/bin/protractor",
            "stopOnEntry": false,
            "args": ["${workspaceRoot}/cucumber/protractor.conf.js", 
                "--params.login.email=", 
                "--params.login.password="],
        },
        {
            "name": "Jasmine",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceRoot}",
            "program": "${workspaceRoot}/node_modules/protractor/bin/protractor",
            "stopOnEntry": false,
            "args": ["${workspaceRoot}/jasmine/protractor.conf.js", 
                "--params.login.email=", 
                "--params.login.password="],
        }
    ]
}