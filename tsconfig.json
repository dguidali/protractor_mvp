{
  "compileOnSave": false,
  "compilerOptions": {
    "target": "es6",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "strict": true,
    "noUnusedLocals": true,
    "module": "commonjs",
    "baseUrl": ".",
    "typeRoots": [
      "./node_modules/@types"
    ],
    "paths": {
      "@pages/*": ["page_objects/*"]
    }
  }
}